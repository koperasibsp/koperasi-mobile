            <RelativeLayout
              android:id="@+id/top5loan"
              android:layout_below="@id/menu_baru"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_marginTop="20dp"
              android:layout_marginBottom="20dp"
              android:background="@color/bgFeatureOne">

              <LinearLayout
                android:id="@+id/top5LoanTitle"
                android:layout_width="135dp"
                android:layout_height="150dp"
                android:background="@color/bgFeatureTwo"
                android:orientation="vertical">

                <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="5 Top Pinjaman"
                  android:textColor="@color/darkbsp"
                  android:textAllCaps="true"
                  android:fontFamily="@font/ubuntu_medium"
                  android:textSize="20dp"
                  android:layout_marginBottom="10dp"
                  android:layout_marginLeft="10dp"
                  android:layout_marginTop="20dp"/>

                <LinearLayout
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:padding="5dp"
                  android:background="@color/bgFeatureThree"
                  tools:layout_conversion_absoluteHeight="51dp"
                  tools:layout_editor_absoluteY="619dp"
                  tools:layout_editor_absoluteX="0dp"
                  tools:layout_conversion_absoluteWidth="135dp">
                <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_marginLeft="10dp"
                  android:textSize="12dp"
                  android:fontFamily="@font/ubuntu"
                  android:text="5 Jenis Pinjaman Terbanyak Diajukan Anggota"
                  android:textColor="@color/darkbsp"/>
                </LinearLayout>
              </LinearLayout>
              <androidx.recyclerview.widget.RecyclerView
                android:paddingTop="5dp"
                android:id="@+id/recyclerviewTopLoan"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:layout_toRightOf="@+id/top5LoanTitle"/>

            </RelativeLayout>


            <TextView
                android:id="@+id/txtView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="5sp"
                android:layout_gravity="center_horizontal"
                android:textColor="@color/darkbsp"
                android:text="2020"/>

            <TextView
              android:id="@+id/privacy_policy"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="center_horizontal"
              android:textColor="@color/darkbsp"
              android:text="Privacy Policy"
              android:layout_marginBottom="15dp"/>


        </LinearLayout>

    </ScrollView>
